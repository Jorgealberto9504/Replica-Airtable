{
  "name": "airtable-clone",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=20.19"
  },
  "scripts": {
    "dev:backend": "npm run -w apps/backend dev",
    "dev:frontend": "npm run -w apps/frontend dev",
    "dev": "npm run dev:backend",
    "dev:all": "npm-run-all -p dev:backend dev:frontend"

  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
